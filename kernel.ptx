.version 7.3
.target sm_52
.address_size 64

.visible .entry foo(
        .param .u64 foo_param_0,
        .param .u64 foo_param_1 
)
{
        .reg .b64       %rd<4>;

        ld.param.u64    %rd1, [foo_param_0];
        ld.param.u64    %rd2, [foo_param_1];
        ld.global.u64   %rd3, [%rd1];
        st.global.u64   [%rd2], %rd3;
        ret;
}